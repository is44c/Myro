<?xml version="1.0" encoding="utf-8"?>
<Manifest 
    xmlns="http://schemas.microsoft.com/xw/2004/10/manifest.html"
    xmlns:dssp="http://schemas.microsoft.com/xw/2004/10/dssp.html"
    >
	
  <!-- Start the standard Scribbler services -->

  <CreateServiceList>

    <!--Start Scribbler Base Service -->
    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblerbase.html</dssp:Contract>
      <dssp:Service>http://hello/1</dssp:Service>
    </ServiceRecordType>

    <!-- Start the Scribbler Drive Service -->
    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblerdrive.html</dssp:Contract>
      <dssp:Service>http://hello/3</dssp:Service>
    </ServiceRecordType>

    <!--Start Scribbler Light Sensors service -->
    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblerlightsensor.html</dssp:Contract>
      <dssp:Service>http://hello/4</dssp:Service>
    </ServiceRecordType>
    
    <!--Start Scribbler Bumper sensor service -->
    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblerbumper.html</dssp:Contract>
      <dssp:Service>http://hello/5</dssp:Service>
    </ServiceRecordType>

    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblertonegenerator.html</dssp:Contract>
        <dssp:Service>http://hello/tone</dssp:Service>
    </ServiceRecordType>

    <ServiceRecordType>
      <dssp:Contract>http://www.roboteducation.org/scribblerledarray.html</dssp:Contract>
      <dssp:Service>http://hello/led</dssp:Service>
    </ServiceRecordType>
    
  </CreateServiceList>

</Manifest>
